--&&상영시스템 (ex.4dx, imax) &&--
CREATE table hicgv_movie_system(
movie_system_id number(20) PRIMARY KEY NOT NULL, 
movie_id number(20) NOT NULL, --movie KEY
system_name varchar2(20) --상영시스템 
)

