--$$영화차트$$-- SJ
CREATE table hicgv_movie( 
movie_id number(5) PRIMARY KEY NOT NULL, --영화ID(상단바주소에5글자숫자)
ranking number(11), --순위
title_kor varchar2(255) NOT NULL,  --영화제목(한글)
title_eng varchar2(255), --영화제목(영어)
opening_date DATE, --개봉일
genre varchar2(40), --장르
nation varchar2(30), --나라
running_time number(11), --상영시간
description varchar2(2555), --영화설명
company_image_url varchar2(11), --배급사로고
age_limit varchar2(24), --연령제한
image_url varchar2(2555), --메인 이미지 경로
audience_total number(11), --누적관람객
audience_today number(11), --오늘관람객
audience_yesterday number(11), --어제관람객
audience_sex_ratio number(11), --관람객 성 비율
audience_age10_ratio number(11), --관람객 10대 비율
audience_age20_ratio number(11), --관람객 20대 비율
audience_age30_ratio number(11), --관람객 30대 비율
audience_age40_ratio number(11), --관람객 40대 비율
audience_rating number(11), --관람평
booking_rate number(11) --예매율
);

SELECT * FROM hicgv_movie;

--$영화차트디비$--
